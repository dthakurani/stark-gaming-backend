{
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js}": [
      "npx eslint . --fix",
      "npx prettier --write .",
      "git add"
    ]
  },
  "name": "stark-gaming",
  "version": "1.0.0",
  "node-version": "v19.6.0",
  "npm-version": "9.4.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "jest",
    "format": "prettier --write \"./**/*.{js,jsx}\"",
    "lint": "npx eslint ./",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "helmet": "^6.1.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "eslint": "^8.39.0"
  }
}
