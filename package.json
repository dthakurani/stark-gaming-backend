{
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js}": [
      "npx eslint . --fix",
      "npx prettier --write .",
      "git add"
    ]
  },
  "name": "stark-gaming",
  "version": "1.0.0",
  "node-version": "v19.6.0",
  "npm-version": "9.4.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest",
    "format": "prettier --write \"./**/*.{js,jsx}\"",
    "lint": "npx eslint ./",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "fernet-nodejs": "^1.0.6",
    "helmet": "^6.1.5",
    "husky": "^8.0.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.2.2",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "prettier": "^2.8.8",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0"
  }
}
